trip()\n","      if skill:\n","        skills_set.add(skill)\n","\n","print('Skills:', skills_set)\n","location = jobs[0].find('div', class_ = 'srp-loc')\n","location.text.strip()\n","locations = []\n","\n","for job in jobs:\n","  location_tag = job.find('div', class_ = 'srp-loc')\n","  if location_tag:\n","    location = location_tag.text.strip()\n","    locations.append(location)\n","\n","print('Locations:', locations[:10])\n","experience = jobs[0].find('div', class_ = 'srp-exp')\n","experience.text.strip()\n","experiences = []\n","\n","for job in jobs:\n","  experience_tag = job.find('div',class_ = 'srp-exp')\n","  if experience_tag:\n","    experience = experience_tag.text.strip()\n","    experiences.append(experience)\n","print('Experience: ',experiences)\n","#to combine everything\n","def job_finder():\n","  key = input(\"Enter you skills or designation: \").strip()\n","  key_url = key.replace(\" \",\"+\")\n","  url = (f\"https://m.timesjobs.com/mobile/jobs-search-result.html?txtKeywords={key}&cboWorkExp1=-1&txtLocation=\")\n","  response = requests.get(url)\n","  soup = BeautifulSoup(response.text)\n","  jobs  = soup.find_all('div',class_ = 'srp-listing clearfix')\n","  company_names = []\n","  skills_list = []\n","  locations = []\n","  experiences = []\n","\n","  '''if not jobs:\n","    print(\"No jobs found with the current selection. you can inspect the HTML: \")\n","    #print(soup.prettify()[:2000])\n","    return [{\n","        \"company Name\": [],\n","        \"skills\": [],\n","        \"location\": [],\n","        \"experience\": []\n","    }]'''\n","  for job in jobs: #company Name\n","    company_tag = job.find('span', class_ = 'srp-comp-name')\n","    if company_tag:\n","      comp_text = company_tag.text.strip()\n","      comp_text = ', '.join(comp_text.split())\n","    else:\n","      comp_text = 'N/A'\n","    company_names.append(comp_text)\n","\n","    #skills\n","    skills_div = job.find('div',class_ ='srp-keyskills')\n","    if skills_div:\n","      skills_tags = skills_div.find_all('a')\n","      skills = [skill.text.strip() for skill in skills_tags]\n","      skills_text = ', '.join(skills)\n","    else:\n","      skills_text = 'N/A'\n","    skills_list.append(skills_text)\n","\n","    #location\n","    location_tag = job.find('div', class_ = 'srp-loc')\n","    if location_tag:\n","      location = location_tag.text.strip()\n","      location = ', '.join(location.split(','))\n","    else:\n","      location = 'N/A'\n","    locations.append(location)\n","\n","    #experience\n","    experience_tag = job.find('div',class_ = 'srp-exp')\n","    if experience_tag:\n","      experience = experience_tag.text.strip()\n","    else:\n","      experience = 'N/A'\n","    experiences.append(experience)\n","\n","  result = {\n","      \"company Name\": company_names,\n","      \"skills\": skills_list,\n","      \"location\": locations,\n","      \"experience\": experiences\n","  }\n","  print(f\"Extracted {len(company_names)} job postings.\")\n","  return result\n","\n","data = job_finder()\n","data.keys()\n","data['experience']\n","data['company Name']\n","data['skills']\n","#taking unique values of skills - for that we use set (set return only unique values )\n","\n","# pandas\n","import pandas as pd\n","df = pd.DataFrame(data)\n","df\n","df.to_excel('jobs.xlsx', index = False)\n"],"metadata":{"id":"ZyFMalLYjUGe"}},{"cell_type":"code","source":["import requests"],"metadata":{"id":"ir2hKobnGN89"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["url = 'https://m.timesjobs.com/'\n","response = requests.get(url)\n","response"],"metadata":{"id":"hjaMhrhlGOBY","executionInfo":{"status":"ok","timestamp":1753092701854,"user_tz":-330,"elapsed":1342,"user":{"displayName":"Jayaprakash Kamireddy","userId":"11558853386970998673"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"eded20de-471b-4576-be9c-7ee97fdd73a3"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"te