skills\n","    skills_div = job.find('div',class_ ='srp-keyskills')\n","    if skills_div:\n","      skills_tags = skills_div.find_all('a')\n","      skills = [skill.text.strip() for skill in skills_tags]\n","      skills_text = ', '.join(skills)\n","    else:\n","      skills_text = 'N/A'\n","    skills_list.append(skills_text)\n","\n","    #location\n","    location_tag = job.find('div', class_ = 'srp-loc')\n","    if location_tag:\n","      location = location_tag.text.strip()\n","      location = ', '.join(location.split(','))\n","    else:\n","      location = 'N/A'\n","    locations.append(location)\n","\n","    #experience\n","    experience_tag = job.find('div',class_ = 'srp-exp')\n","    if experience_tag:\n","      experience = experience_tag.text.strip()\n","    else:\n","      experience = 'N/A'\n","    experiences.append(experience)\n","\n","  result = {\n","      \"company Name\": company_names,\n","      \"skills\": skills_list,\n","      \"location\": locations,\n","      \"experience\": experiences\n","  }\n","  print(f\"Extracted {len(company_names)} job postings.\")\n","  return result\n"],"metadata":{"id":"texrjLoLuX4O"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data = job_finder()"],"metadata":{"id":"WrGLgvlnTUyP","executionInfo":{"status":"ok","timestamp":1753094957007,"user_tz":-330,"elapsed":11494,"user":{"displayName":"Jayaprakash Kamireddy","userId":"11558853386970998673"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"28ea28a7-6d39-4ef5-8f6f-d7c8db4ccf12"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Enter you skills or designation: Python Programming\n","Extracted 25 job postings.\n"]}]},{"cell_type":"code","source":["data.keys()"],"metadata":{"id":"IAfMb128uYBi","executionInfo":{"status":"ok","timestamp":1753094958580,"user_tz":-330,"elapsed":101,"user":{"displayName":"Jayaprakash Kamireddy","userId":"11558853386970998673"}},"colab":{"base_uri":"https://localhos